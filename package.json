    "hap-node-client": "^0.1.5",
    "hap-nodejs": "^0.8.2",
    "homebridge": "^1.3.3",
    "nodemon": "^2.0.5",
    "request-promise": "^4.2.6",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
