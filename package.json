    "build:ui": "npm run build --prefix homebridge-ui/public",
    "lint": "eslint --ext .ts --max-warnings=0 .",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">=1.0.0"
  },
  "keywords": [
    "homebridge-plugin",
    "google smart home",
    "google-smart-home",
    "google home",
    "google-home",
    "google assistant",
    "google-assistant",
    "google",
    "android",
    "homebridge",
    "touchwand"
  ],
  "bundledDependencies": [
    "@oznu/ws-connect",
    "fs-extra",
    "hap-node-client",
    "rxjs"
  ],
  "dependencies": {
    "@oznu/ws-connect": "^2.2.0",
    "fs-extra": "^9.0.1",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.2",
    "@types/node": "^14.11.8",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
